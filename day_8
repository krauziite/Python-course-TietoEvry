#2.
def replace_item(d, bad_val, good_val):
    for key, val in d:
        if val==bad_val:
            d[val] = good_val
        print(replace_item({'a': 5, 'b': 6, 'c': 5}, 5, 10))
    return replace_item({'a': 5, 'b': 6, 'c': 5}, 5, 10)
