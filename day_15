import sqlite3

def create_connection(dbpath):
    conn=sqlite3.connect(dbpath)
    cur=conn.cursor()
    return cur

create_connection('chinook.db')


def create_artist(cur, artist_name):
    cur.execute('INSERT INTO albums (Title, ArtistId) VALUES (?, ?)',
                artist_name)
    return cur.fetchone()


def read_artists(cur):
    row_it=cur.execute('SELECT * FROM albums LIMIT 10')
    results_list = [row for row in row_it]  # just list(row_it) would work here as well
    print(results_list)
